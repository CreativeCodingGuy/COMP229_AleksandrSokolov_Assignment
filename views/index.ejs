<%- include partials/content.ejs %>

<!-- Show the template of the respective page using if conditionals -->

  <% if (title == "Home" || title == "/") { %>
    <%- include partials/homepage.ejs %>
  <% } %>

  
  <% if (title == "About Me") { %>
    <%- include partials/aboutpage.ejs %>
  <% } %>

  <% if (title == "Projects") { %>
    <%- include partials/projectspage.ejs %>
  <% } %>

  <% if (title == "Services") { %>
    <%- include partials/servicespage.ejs %>
  <% } %>

  <% if (title == "Contacts") { %>
    <%- include partials/contactmepage.ejs %>
  <% } %>

<!-- Including bottomnav and footer partials -->
<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>

